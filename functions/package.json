{
  "name": "bugyo-agent-functions",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "build:prod": "npm run build && npm prune --production",
    "deploy:prod": "npm run build:prod && npm run publish",
    "watch": "tsc --w",
    "prestart": "npm run build && func extensions install",
    "start:host": "func start",
    "start": "npm-run-all --parallel start:host watch",
    "test": "echo \"No tests yet...\"",
    "publish": "func azure functionapp publish $npm_config_function_name"
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "^1.0.1-beta1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "bugyo-cloud-client": "^0.2.0"
  }
}
